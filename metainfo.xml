<?xml version="1.0" encoding="utf-8"?>
<Extension>

	<name>doccenter</name>
	<version>0.1</version>
	<title>文档中心</title>
	<description>
通过反射PHP类生成 API文档，扫描源代码中的注释生成 Wiki文档 和 例子。
	</description>

	<class>org.opencomb.doccenter.DocCenter</class>
	<package folder="/class" namespace="org.opencomb.doccenter" />

	<template folder="/template" />

	<publicFolder folder="/public" />

	<beanFolder folder="/bean" />

	<require type="extension" item="coresystem"><![CDATA[ =0.1 ]]></require>
	<require type="extension" item="advancedcomponent"><![CDATA[ =0.1 ]]></require>
	
</Extension>